{
  "name": "bozo-reactjs-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate code for ReactJS + Redux + Less + Controlled form + AJAX + HTTP request interceptor + Router v4 + Authentication",
  "homepage" : "http://www.srpskibre.com/react",
  "main": "src/index.js",
  "scripts": {
    "build-css": "less-watch-compiler --run-once --main-file=App.less src/ src/",
    "watch-css": "npm run build-css && less-watch-compiler --main-file=App.less src/ src/",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "prebuild": "rimraf dist"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "redux",
    "less",
    "form",
    "ajax",
    "interceptor",
    "router",
    "authentication"
  ],
  "author": "Bozo Jovicic",
  "user": "bozojovicic",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "axios": "^0.18.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.0",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "less-watch-compiler": "^1.11.0",
    "npm-run-all": "^4.1.2",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss-loader": "^2.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "uglify-js-brunch": "^2.10.0",
    "webpack": "^1.13.2",
    "webpack-cli": "^2.0.10",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-server": "1.16.2"
  }
}
